 "html": "\n<!--  include \"art_collections/art_collections/print_format/art_pr/art_pr.html\"  -->\n\n{% set ctx = get_print_context_for_art_collections_purchase_receipt(doc.doctype, doc.name) %}\n\n<style>\n    .bordered>.row {\n        border: 1px solid #eee !important;\n        padding-top: 3px;\n    }\n\n    .bordered>.row>.col {\n        border-right: 1px solid #eee !important;\n    }\n\n    table {\n        font-size: 9px !important;\n    }\n\n    th {\n        font-size: 9px !important;\n    }\n\n    table td {\n        padding: 8px 0;\n    }\n</style>\n\n<div class=\"row\" style=\"padding-top:0.5rem;\">\n    <div class=\"col-xs-2 logodiv\">\n        <img src=\"/files/logo01.png\" class=\"logofirst\" />\n    </div>\n    <div class=\"col-xs-6 logodiv\">\n        <img src=\"/files/logo02.png\" class=\"logosec\" />\n    </div>\n    <div class=\"col-xs-4\" style=\"text-align:right\">\n        <p>{{ _(\"Purchase Receipt\") }}</p>\n        <p>{{ doc.name }} </p>\n    </div>\n</div>\n\n\n<div class=\"row\">\n    <div class=\"col-xs-12\">\n        <table class=\"table table-condensed table-hover table-bordered\" style=\"margin-top:10\">\n            <colgroup>\n                <col style=\"width:2%\">\n                <col style=\"width:73%\">\n                <col style=\"width:15%\">\n                <col style=\"width:10%\">\n            </colgroup>\n            <thead>\n                <tr>\n                    <th>{{ _(\"No\") }} </th>\n                    <th>{{ _(\"Item Name\") }} </th>\n                    <th>{{ _(\"Qty of Outer\") }} </th>\n                    <th>{{ _(\"Pallet Qty\") }} </th>\n                </tr>\n            </thead>\n            <tbody>\n                {% for d in ctx.get(\"items\",[]) %}\n                <tr>\n                    <td>{{ loop.index }}</td>\n                    <td>{{d.item_code}}:{{ d.item_name}}</td>\n                    <td class=\"text-right\">{{ frappe.utils.cint(d.qty_of_outer) }}</td>\n                    <td class=\"text-right\">{{ frappe.utils.cint(d.pallet_qty) }}</td>\n                </tr>\n                {% endfor %}\n            </tbody>\n        </table>\n    </div>\n</div>\n\n{% if set_apart_po_item_for_customer_cf %}\n<span>Set Apart Items</span>\n<div class=\"row\">\n    <div class=\"col-xs-12\">\n        <table class=\"table table-condensed table-hover table-bordered\" style=\"margin-top:10\">\n            <colgroup>\n                <col style=\"width:2%\">\n                <col style=\"width:43%\">\n                <col style=\"width:20%\">\n                <col style=\"width:35%\">\n            </colgroup>\n            <thead>\n                <tr>\n                    <th>{{ _(\"Sr\") }} </th>\n                    <th>{{ _(\"Item Name\") }} </th>\n                    <th>{{ _(\"Qty\") }} </th>\n                    <th>{{ _(\"Customer\") }} </th>\n                </tr>\n            </thead>\n            <tbody>\n                {% for row in doc.set_apart_po_item_for_customer_cf %}\n                <tr>\n                    <td>{{ row.idx }}</td>\n                    <td>{{row.item_code}}</td>\n                    <td class=\"text-right\">{{ row.qty }}</td>\n                    <td class=\"text-right\">{{ row.customer }}</td>\n                </tr>\n                {% endfor %}\n            </tbody>\n        </table>\n    </div>\n</div>\n{% endif  %}\n\n{% if spl_unknown_item_cf %}\n<span>Supplier Packing List Unknown Item</span>\n<div class=\"row\">\n    <div class=\"col-xs-12\">\n        <table class=\"table table-condensed table-hover table-bordered\" style=\"margin-top:10\">\n            <colgroup>\n                <col style=\"width:2%\">\n                <col style=\"width:73%\">\n                <col style=\"width:15%\">\n                <col style=\"width:10%\">\n            </colgroup>\n            <thead>\n                <tr>\n                    <th>{{ _(\"Sr\") }} </th>\n                    <th>{{ _(\"Item Name\") }} </th>\n                    <th>{{ _(\"Supplier Item Code\") }} </th>\n                    <th>{{ _(\"Qty\") }} </th>\n                </tr>\n            </thead>\n            <tbody>\n                {% for row in doc.spl_unknown_item_cf %}\n                <tr>\n                    <td>{{ row.idx }}</td>\n                    <td>{{row.item_code}} :{{row.item_name}}  </td>\n                    <td class=\"text-right\">{{ row.supplier_item_code }}</td>\n                    <td class=\"text-right\">{{ row.qty_as_per_spl }}</td>\n                </tr>\n                {% endfor %}\n            </tbody>\n        </table>\n    </div>\n</div>\n{% endif  %}\n",
