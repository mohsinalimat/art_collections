 "html": "<!-- include \"art_collections/art_collections/print_format/art_pick/art_pick.html\" -->\n\n\n{% set ctx = get_print_context_for_art_collectons_pick_list(doc.doctype, doc.name) %}\n\n<style>\n    .bordered>.row {\n        border: 1px solid #eee !important;\n        padding-top: 3px;\n    }\n\n    .bordered>.row>.col {\n        border-right: 1px solid #eee !important;\n    }\n\n    table {\n        font-size: 9px !important;\n    }\n\n    th {\n        font-size: 6px !important;\n    }\n\n    table td {\n        padding: 8px 0;\n    }\n</style>\n\n<div class=\"row\" style=\"padding-top:0.5rem;\">\n    <div class=\"col-xs-2 logodiv\">\n        <img src=\"/files/logo01.png\" class=\"logofirst\" />\n    </div>\n    <div class=\"col-xs-6 logodiv\">\n        <img src=\"/files/logo02.png\" class=\"logosec\" />\n    </div>\n    <div class=\"col-xs-4\" style=\"text-align:right\">\n        <p>{{ _(\"PICK LIST\") }}</p>\n        <p>{{ doc.name }} </p>\n    </div>\n</div>\n\n<div class=\"master\">\n    <table class=\"table table-condensed table-hover table-bordered\">\n        <colgroup>\n            <col style=\"width:20%\">\n            <col style=\"width:30%\">\n            <col style=\"width:20%\">\n            <col style=\"width:30%\">\n        </colgroup>\n        <tr>\n            <td>Purpose</td>\n            <td>Delivery</td>\n            <td>Double Check</td>\n            <td>{{ctx.doc.double_check_order_flag_art or \"\"}}</td>\n        </tr>\n        <tr>\n            <td> Customer Name </td>\n            <td> {{ doc.customer_name_art or \"\"}} </td>\n            <td> Address Title </td>\n            <td> {{ ctx.get(\"doc\",{})[\"shipping_address_name\"] or \"\" }} </td>\n        </tr>\n        <tr>\n            <td> Address Line 1</td>\n            <td> {{ ctx.get(\"doc\",{})[\"address_line1\"] or \"\" }}</td>\n            <td> Address Line 2</td>\n            <td> {{ ctx.get(\"doc\",{})[\"address_line2\"] or \"\" }}</td>\n        </tr>\n        <tr>\n            <td> Zip Code</td>\n            <td> {{ ctx.get(\"doc\",{})[\"pincode\"] or \"\" }}</td>\n            <td> City Town</td>\n            <td> {{ ctx.get(\"doc\",{})[\"city\"] or \"\" }}</td>\n        </tr>\n        <tr>\n            <td>Delivery Prior Appointment</td>\n            <td>{% if doc.delivery_by_appointment_art %} Yes {% else %} No {% endif %}</td>\n            <td>Country</td>\n            <td>{{ ctx.get(\"doc\",{})[\"country\"] or \"\" }}</td>\n        </tr>\n        {% if doc.delivery_appointment_contact_detail_art %}\n        <tr>\n            <td colspan=\"4\">\n                <p>{{ _(\"Delivery Prior Appointment Detail\") }}<br>{{ doc.delivery_appointment_contact_detail_art or \"\"\n                    }}\n                </p>\n            </td>\n        </tr>\n        {% endif %}\n        {% if doc.directive_print_art %}\n        <tr>\n            <td colspan=\"4\">\n                <p>{{ _(\"Print Directive\") }}<br>{{ doc.directive_print_art or \"\" }}</p>\n            </td>\n        </tr>\n        {% endif %}\n\n    </table>\n</div>\n\n<div class=\"row\">\n    <div class=\"col-xs-12\">\n        <table class=\"table table-condensed table-hover table-bordered\" style=\"margin-top:10\">\n            <colgroup>\n                <col style=\"width:3\">\n                <col style=\"width:36\">\n                <col style=\"width:9\">\n                <col style=\"width:6\">\n                <col style=\"width:6\">\n                <col style=\"width:6\">\n                <col style=\"width:6\">\n                <col style=\"width:6\">\n                <col style=\"width:10\">\n                <col style=\"width:6\">\n                <col style=\"width:6\">\n\n            </colgroup>\n            <thead>\n                <tr>\n                    <th>{{ _(\"No\") }} </th>\n                    <th>{{ _(\"Item Name\") }} </th>\n                    <th>{{ _(\"WH\") }} </th>\n                    <th>{{ _(\"Qty\") }} </th>\n                    <th>{{ _(\"UOM\") }} </th>\n                    <th>{{ _(\"CF\") }} </th>\n                    <th>{{ _(\"Stock UOM\") }} </th>\n                    <th>{{ _(\"Order\") }} </th>\n                    <th>{{ _(\"Delivery\") }} </th>\n                    <th>{{ _(\"Facing\") }} </th>\n                    <th>{{ _(\"PO\") }} </th>\n                </tr>\n            </thead>\n            <tbody>\n                {% for d in ctx.get(\"items\",[]) %}\n                <tr>\n                    <td>{{ loop.index }}</td>\n                    <td>{{d.item_code}}:{{ d.item_name}}</td>\n                    <td>{{ d.warehouse or \"\" }}</td>\n                    <td class=\"text-right\">{{ d.qty }}</td>\n                    <td>{{ d.uom }}</td>\n                    <td class=\"text-right\">{{ d.conversion_factor }}</td>\n                    <td>{{ d.stock_uom }}</td>\n                    <td>{{ d.sales_order or \"\" }}</td>\n                    <td>{{ d.delivery_date or \"\" }}</td>\n                    <td>{{ d.facing_required_art or \"No\" }}</td>\n                    <td>{{ d.po_no or \"&nbsp;\" }}</td>\n                </tr>\n                {% endfor %}\n            </tbody>\n        </table>\n    </div>\n</div>",
